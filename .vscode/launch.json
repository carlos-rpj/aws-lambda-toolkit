  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "List Stacks",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "${workspaceFolder}/index.js",
        "list-stacks"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "List Resources",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "${workspaceFolder}/index.js",
        "list-resources",
        "resource-name",
        "--only-logical-id"
      ]
    }
  ]
}